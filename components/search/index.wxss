.ty-search {
  font-size: 28rpx;
  color: #666;
}
.ty-search__head {
  display: flex;
  padding: 0 30rpx;
  height: 106rpx;
  align-items: center;
  justify-content: space-between;
  border-top: 1rpx #f5f5f5 solid;
  border-bottom: 1rpx #f5f5f5 solid;
}
.ty-search__head .input-group {
  display: flex;
  align-items: center;
  padding: 0 15rpx 0 30rpx;
  box-sizing: border-box;
  width: 540rpx;
  height: 68rpx;
  background-color: #f7f7f7;
  border-radius: 34rpx;
}

.ty-search__head .icon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}

.ty-search__head .input {
  flex: 1;
}

.ty-search__head .cancel {
  width: 24rpx;
  height: 24rpx;
  padding: 15rpx;
}

.ty-search__head .btn {
  width: 120rpx;
  border: 1rpx solid #666;
  height: 66rpx;
  border-radius: 34rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ty-search-body {
  padding: 0 30rpx;
}

.ty-search-body .tag{
  margin: 0 10rpx 20rpx;
}

.ty-search-body .mt20 {
  margin-top: 20rpx;
}