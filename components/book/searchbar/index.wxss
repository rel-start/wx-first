.ty-searchbar {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  z-index: 9;
  display: flex;
  justify-content: center;
  height: 108rpx;
  align-items: center;
  background-color: #fff;
  border-top: 1rpx #f5f5f5 solid;
}

.ty-searchbtn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 690rpx;
  height: 68rpx;
  border-radius: 34rpx;
  background-color: #f7f7f7;
  color: #999;
  font-size: 28rpx;
}

.ty-searchbtn-icon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}